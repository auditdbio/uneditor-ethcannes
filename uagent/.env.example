# A seed phrase for the uAgent. If not provided, a random one will be used.
SEED_PHRASE="my super secret uagent seed phrase"

# The full base URL of the FastAPI server from the 'agent' project
AGENT_SERVER_URL="http://127.0.0.1:8000"

# The secret key to authenticate with the FastAPI server.
# This MUST match the AGENT_API_SECRET_KEY in the server's .env file.
AGENT_API_SECRET_KEY="change-me-to-a-super-secret-key" 